  CPP="${CC-cc} -nologo -E"
  cat > conftest.$ac_ext <<EOF
#line 2484 "configure"
#include "confdefs.h"
#include <assert.h>
Syntax Error
EOF
ac_try="$ac_cpp conftest.$ac_ext >/dev/null 2>conftest.out"
